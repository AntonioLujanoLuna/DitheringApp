{
  "buildCommand": "node scripts/vercel-build.mjs",
  "framework": "vite",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "DISABLE_PWA": "true",
    "NODE_ENV": "production"
  }
}