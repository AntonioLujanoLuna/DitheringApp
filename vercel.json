{
  "buildCommand": "ROLLUP_NATIVE_EXTENSIONS=false vite build --config vite.vercel.config.js",
  "framework": "vite",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "DISABLE_PWA": "true",
    "NODE_ENV": "production",
    "ROLLUP_NATIVE_EXTENSIONS": "false"
  }
}