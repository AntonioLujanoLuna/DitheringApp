{
    "buildCommand": "npm install --no-optional && npm run build",
    "framework": "vite",
    "outputDirectory": "dist",
    "installCommand": "npm install --no-optional",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "env": {
      "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
    }
  }