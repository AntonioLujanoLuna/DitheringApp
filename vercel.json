{
  "buildCommand": "npm run vercel-build",
  "framework": "vite",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "DISABLE_PWA": "true",
    "NODE_ENV": "production",
    "ROLLUP_NATIVE_EXTENSIONS": "false",
    "VERCEL": "1"
  },
  "buildEnvironment": {
    "DISABLE_PWA": "true",
    "NODE_ENV": "production",
    "ROLLUP_NATIVE_EXTENSIONS": "false",
    "VERCEL": "1"
  }
}