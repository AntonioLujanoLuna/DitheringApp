{
    "buildCommand": "node scripts/vercel-build-minimal.js",
    "framework": "vite",
    "outputDirectory": "dist",
    "installCommand": "npm install --production --no-package-lock",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "env": {
      "DISABLE_PWA": "true",
      "NODE_ENV": "production"
    }
  }